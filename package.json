{
  "name": "gana-con-la-cantrina",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "export": "next build",
    "lint": "next lint",
    "serve": "npx serve out -p 3000",
    "build:static": "next build",
    "netlify:build": "npm run validate-env && next build",
    "netlify:dev": "next dev",
    "postbuild": "echo 'Build completed successfully for Netlify deployment'",
    "clean": "rmdir /s /q .next 2>nul || rmdir /s /q out 2>nul || rmdir /s /q dist 2>nul || echo cleaned",
    "prebuild": "npm run clean",
    "validate-env": "node scripts/validate-env.js",
    "typescript-check": "node scripts/typescript-safety-check.js",
    "prebuild-safe": "npm run clean && npm run typescript-check",
    "build-safe": "npm run prebuild-safe && next build",
    "debug-env": "node -e \"console.log('SUPABASE_URL:', process.env.NEXT_PUBLIC_SUPABASE_URL?.substring(0,30) + '...'); console.log('ANON_KEY:', process.env.NEXT_PUBLIC_SUPABASE_ANON_KEY?.substring(0,20) + '...');\"",
    "diagnose-netlify": "node scripts/diagnose-netlify.js",
    "debug-build": "npm run build && npm run diagnose-netlify"
  },
  "dependencies": {
    "@netlify/functions": "4.2.2",
    "@netlify/plugin-nextjs": "^5.13.0",
    "@react-spring/web": "^10.0.1",
    "@supabase/ssr": "^0.7.0",
    "@supabase/supabase-js": "^2.56.0",
    "@tailwindcss/forms": "^0.5.10",
    "@tailwindcss/postcss": "^4.1.12",
    "@types/bcryptjs": "^2.4.6",
    "@types/node": "^20.19.11",
    "@types/react": "^18.3.24",
    "@types/react-dom": "^18.3.7",
    "@types/react-window": "^1.8.8",
    "autoprefixer": "^10.4.21",
    "bcryptjs": "^3.0.2",
    "claude": "^0.1.1",
    "clsx": "^2.1.1",
    "dotenv": "^17.2.2",
    "framer-motion": "^12.23.12",
    "lucide-react": "^0.540.0",
    "next": "^15.4.4",
    "postcss": "^8.4.21",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-hot-toast": "^2.6.0",
    "react-window": "^1.8.11",
    "sharp": "^0.34.3",
    "tailwind-merge": "^3.3.1",
    "tailwindcss": "^3.3.5",
    "tesseract.js": "^6.0.1",
    "typescript": "^5.9.2",
    "zustand": "^4.5.7"
  },
  "engines": {
    "node": ">=18.17.0",
    "npm": ">=8.0.0"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "eslint": "^8.57.1",
    "eslint-config-next": "^15.5.4"
  }
}
